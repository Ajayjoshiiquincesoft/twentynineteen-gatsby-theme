{
  "name": "twentynineteen-gatsby-theme",
  "version": "0.1.0",
  "description": "The Gatsby Theme - ported from Twenty Nineteen WordPress theme.",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "esm": "^3.2.10",
    "gatsby": "^2.1.23",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "scripts": {
