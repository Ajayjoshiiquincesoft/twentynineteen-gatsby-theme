{
  "name": "starter",
  "version": "0.1.0",
  "description": "The Starter Site to test the Twenty Nineteen Gatsby Theme.",
  "main": "gatsby-config.js",
  "license": "MIT",
  "scripts": {
    "build": "npx --node-arg \"-r esm\" gatsby build",
    "develop": "npx --node-arg \"-r esm\" gatsby develop",
    "start": "npx --node-arg \"-r esm\" npm run develop",
    "serve": "npx --node-arg \"-r esm\" gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "gatsby": "^2.1.23",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "twentynineteen-gatsby-theme": "*"
  }
}
